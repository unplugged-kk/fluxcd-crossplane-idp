{
    "": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "kubernetesClusters": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "providerConfigRefName": {
                        "type": "string"
                    },
                    "location": {
                        "type": "string"
                    },
                    "resourceGroupName": {
                        "type": "string"
                    },
                    "outboundType": {
                        "type": "string"
                    },
                    "kubernetesVersion": {
                        "type": "string"
                    },
                    "privateClusterEnabled": {
                        "type": "boolean"
                    },
                    "dnsPrefix": {
                        "type": "string"
                    },
                    "automaticChannelUpgrade": {
                        "type": "string"
                    },
                    "httpApplicationRoutingEnabled": {
                        "type": "boolean"
                    },
                    "runCommandEnabled": {
                        "type": "boolean"
                    },
                    "identity": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "identityIds": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                },
                                "type": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "identityIds",
                                "type"
                            ]
                        }
                    },
                    "networkProfile": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "dnsServiceIp": {
                                    "type": "string"
                                },
                                "networkMode": {
                                    "type": "string"
                                },
                                "networkPlugin": {
                                    "type": "string"
                                },
                                "networkPolicy": {
                                    "type": "string"
                                },
                                "serviceCidrs": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            },
                            "required": [
                                "dnsServiceIp",
                                "networkPlugin",
                                "networkPolicy",
                                "serviceCidrs"
                            ]
                        }
                    },
                    "defaultNodePool": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "nodeCount": {
                                    "type": "integer"
                                },
                                "minCount": {
                                    "type": "integer"
                                },
                                "maxCount": {
                                    "type": "integer"
                                },
                                "maxPods": {
                                    "type": "integer"
                                },
                                "vmSize": {
                                    "type": "string"
                                },
                                "enableAutoScaling": {
                                    "type": "boolean"
                                },
                                "temporaryNameForRotation": {
                                    "type": "string"
                                },
                                "type": {
                                    "type": "string"
                                },
                                "vnetSubnetId": {
                                    "type": "string"
                                },
                                "upgradeSettings": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "maxSurge": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                },
                                "zones": {
                                    "type": "array",
                                    "items": {
                                        "type": "string"
                                    }
                                }
                            },
                            "required": [
                                "name",
                                "nodeCount",
                                "vmSize",
                                "enableAutoScaling",
                                "vnetSubnetId"
                            ]
                        }
                    }
                },
                "required": [
                    "name",
                    "providerConfigRefName",
                    "location",
                    "resourceGroupName",
                    "kubernetesVersion",
                    "privateClusterEnabled",
                    "dnsPrefix",
                    "automaticChannelUpgrade",
                    "identity",
                    "networkProfile",
                    "defaultNodePool"
                ]
            },
            "required": [
                "namespace",
                "resourceGroup"
            ]
        }
    }
}